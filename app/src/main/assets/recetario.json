    "Pasos": [
        {
            "nombre": "Jkuö̀ tchèke, ikuli’ ska i wö̀ñè̠eke, ña̠’ bièketsa̠ e̠na̠ i ulà wö̀́ñè̠kelor, \nès añi̠ès  i ulà e̠na̠ ikalö̀ ajkuö̀ tchéeke.",
            "imagen": "carne_asada1",
            "audio": "carneasada1"
        },
        {
            "nombre": "I wö̀ ttèkelor bitsì̠tsì̠ dicháë. ",
            "imagen": "carne_asada2",
            "audio": "carneasada2"
        },
        {
            "nombre": "Ès i síwè̠ke.",
            "imagen": "carne_asada3",
            "audio": "carne_asada3"
        },
        {
            "nombre": "Kí̠ka sík mè̠ka̠.",
            "imagen": "carne_asada4",
            "audio": "carneasada4"
        },
        {
            "nombre": "Dikì̠ a̠ bö́ ièke.",
            "imagen": "carne_asada5",
            "audio": "carneasada5"
        },
        {
            "nombre": "I kö̀nè̠ke eriàrma̠ kë̀ i ñò̠rwa̠, chkè táne̠wa̠  má̠t e'ta̠ i síne̠.",
            "imagen": "carne_asada6",
            "audio": "carneasada6"
        },
        {
            "nombre": "Kö́ ttö́ièkewa̠ sík wa e’ a̠ chkè e’ ièkèka̠.",
            "imagen": "carne_asada7",
            "audio": "carne_asada7"
        },
        {
            "nombre": "Ès i kichàiö̀ka bö́ tsá̠.",
            "imagen": "canasta_carne",
            "audio": "canastacarne"
        },
        {
            "nombre": "Kö̀chi wö́kir páiklèe bua’ë, tchée shu̠shò̠ ̀",
            "imagen": "chanchopasos1",
            "audio": "chanchopasos1"
        },
        {
            "nombre": "I yík shò̠o̠ i bikèke pápaka̠ tabè wa.",
            "imagen": "chanchopasos2",
            "audio": "chanchopasos2"
        },
        {
            "nombre": "Ièkeka̠ u̠ ̀ bë́rie ése a̠ .",
            "imagen": "chanchopasos3",
            "audio": "chanchopasos3"
        },
        {
            "nombre": "A̠ kalö́m jkuö̀ tchèkelor ákie e’ ièkeka̠ .",
            "imagen": "chanchopasos4",
            "audio": "chanchopasos4"
        },
        {
            "nombre": "Ès i méka̠ bö́ ki̠ nañée̠ ; a̠ bö́ièke ena di’ tèke.",
            "imagen": "chanchopasos5",
            "audio": "chanchopasos5"
        },
        {
            "nombre": "Dö mìka̠ ka̠ ́ ñìne̠bitu̠tche e’ta̠ bö́ yèe i diki̠ ̀ a̠, ès i me̠a̠t dö bulàmi̠.",
            "imagen": "chanchopasos6",
            "audio": "chanchopasos6"
        },
        {
            "nombre": "Bulàmi̠ ù̠ éwa bö tchése̠ í̠s ki̠.",
            "imagen": "chanchopasos7",
            "audio": "chanchopasos7"
        },
        {
            "nombre": "Se’ wák i kèkeka̠ , e'ta̠ chkè albáne̠tsa̠ e̠na̠ klö́m mi̠a ma̠t, ès ta̠ bua’ i alìne̠ ",
            "imagen": "chanchopasos8",
            "audio": "chanchopasos8"
        },
        {
            "nombre": "Be’ na̠mà̠ klö̀we̠wa̠ i ñá̠bita ké̠r, ulà ë̀t wa i so’kuö̀ klèke i wö́kir ké̠r tabèla wa, \nkë̀ i pík tèku̠lor.",
            "imagen": "pescadopasos1",
            "audio": "pescadopasos1"
        },
        {
            "nombre": "Be’ i ñá̠ biétsa̠ , ñá̠ yétsa̠ .",
            "imagen": "pescadopasos2",
            "audio": "pescadopasos2"
        },
        {
            "nombre": "Be’ i alèkeka̠ diö̀ ie, e’ wö̀tèkelor; síwe̠ e’ me̠keka̠ , wáñe̠ , e̠na̠ i kuèke \nsík a̠ bö́ a̠ e’ bikèkewa̠ wáñe̠ añìes.",
            "imagen": "pescadopasos3",
            "audio": "pescadopasos3"
        },
        {
            "nombre": "Pík e̠na̠ ña̠bita yèkelor e̠rpa i duwó e’ta̠ .",
            "imagen": "avespasos1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kuö́ki̠ be’ i ni̠è̠wa di’ba a̠ .",
            "imagen": "avespasos2",
            "audio": "avespasos2"
        },
        {
            "nombre": "Ulà wa be’ i kö̀ yèe.",
            "imagen": "avespasos3",
            "audio": "avespasos3"
        },
        {
            "nombre": "I kö̀yökla tsídalala e’ ñá̠wè̠keka̠ bö́ ki̠",
            "imagen": "avespasos4",
            "audio": "avespasos4"
        },
        {
            "nombre": "I klátskuö tèlor bua’.",
            "imagen": "avespasos5",
            "audio": "avespasos5"
        },
        {
            "nombre": "I kàbita e̠ na̠ i drìi tèlor.",
            "imagen": "avespasos6",
            "audio": "avespasos6"
        },
        {
            "nombre": "Be’ i ña̠ ́ biétsa̠ , e’ mu̠le̠ ́ e’ kukèkeka̠ kiö̀ wa.",
            "imagen": "avespasos7",
            "audio": "avespasos7"
        },
        {
            "nombre": "I ma̠lè wö̀tèkelor i dichéë.",
            "imagen": "avespasos8",
            "audio": "avespasos8"
        },
        {
            "nombre": "E’ alekèka̠ chu̠mu̠ ̀ a̠ o dikó rpö̀ a̠ .",
            "imagen": "avespasos9",
            "audio": "avespasos9"
        },
        {
            "nombre": "Be’ i yuwa’ tchèlor, i wö́kir e̠na̠ i krë̀ kë̀ katàno̠k",
            "imagen": "tortugapasos1",
            "audio": "tortugapasos1"
        },
        {
            "nombre": "Apà ñówè̠ka̠",
            "imagen": "tortugapasos2",
            "audio": "tortugapasos2"
        },
        {
            "nombre": "Apà iklèene̠ ma̠nè̠nè̠.",
            "imagen": "tortugapasos3",
            "audio": "tortugapasos3"
        },
        {
            "nombre": "I chkà wö̀tèlor.",
            "imagen": "tortugapasos4",
            "audio": "tortugapasos4"
        },
        {
            "nombre": "Mèka̠ di’ ta̠ ; a̠ má̠na̠wö ièka̠ ; ès i alèke.",
            "imagen": "tortugapasos5",
            "audio": "tortugapasos5"
        },
        {
            "nombre": "Ù̠ a̠ di’ tèkeka̠ tá̠i̠",
            "imagen": "udio1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Mè̠ka̠ bö́ ki̠",
            "imagen": "udio2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Chkè wö̀tèlor mo̠lò̠lò̠ ",
            "imagen": "udio3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tchée be’ u̠ ̀ ñe̠ ’ a̠ ",
            "imagen": "udio4",
            "audio": "avespasos1"
        },
        {
            "nombre": "A̠ dayë̀ tchée.",
            "imagen": "udio5",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ a̠ tu’ jkuö̀ tchée e’ ièka̠ o cha̠mù̠ jkuö̀ tchée e’ iéka̠.",
            "imagen": "udio6",
            "audio": "avespasos1"
        },
        {
            "nombre": "Na̠mà̠ i di ema̠ e’ alèke i wák ë̀.",
            "imagen": "udio7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kàsir e̠na̠ tsawì e’ alèke má̠na̠wö a̠.",
            "imagen": "udio8",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ yulèke kañìk a̠ e’e̠ se’ i alák tchéeke.",
            "imagen": "pacaya_paso1",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ tchèkewa̠ bö́ a̠ .",
            "imagen": "pacaya_paso2",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ trèke.",
            "imagen": "pacaya_paso3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Mìka̠ i katùne̠ e’ta̠ i jkuö̀lit yétsa̠ .",
            "imagen": "pacaya_paso4",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ta̠ irpö́la katùne̠ e’ katèke.",
            "imagen": "pacaya_paso5",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ kö̀ alö́lö shtèke.",
            "imagen": "calulu_paso1",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ pàléeke ké̠wek.",
            "imagen": "calulu_paso2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kuö̀ki̠ ta̠ e’ kö̀ pàiklèke i kicha yóktsa̠ .",
            "imagen": "calulu_paso3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kuö̀ki̠ ta̠ i wö̀tèkelor mo̠lò̠lò̠ .",
            "imagen": "calulu_paso4",
            "audio": "avespasos1"
        },
        {
            "nombre": "Mé̠ka̠ne ù̠ ki̠.",
            "imagen": "calulu_paso5",
            "audio": "avespasos1"
        },
        {
            "nombre": "A̠ di’ téka̠ e̠na̠ diyë̀ tchéka̠ .",
            "imagen": "calulu_paso6",
            "audio": "avespasos1"
        },
        {
            "nombre": "I katàrmi e’s ë̀ e̠na̠ i wö̀tè̠rmi̠ka̠ krò si̠o̠ ’ ta̠ .",
            "imagen": "calulu_paso7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Bitrèkewa̠ sík wa.",
            "imagen": "quelite_paso1",
            "audio": "avespasos1"
        },
        {
            "nombre": "A̠ diyë̀ tchéka̠.",
            "imagen": "quelite_paso2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Terréwa̠ bua’ë albó.",
            "imagen": "quelite_paso3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tchéwa̠ bö́ a̠ .",
            "imagen": "quelite_paso4",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ trèke eriàrma̠ kë̀ i ñò̠rwa̠ .",
            "imagen": "quelite_paso5",
            "audio": "avespasos1"
        },
        {
            "nombre": "Sík kutùne̠wa̠ e’ta̠ i kutùne̠ , e’s i yèketsa̠ .",
            "imagen": "quelite_paso6",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ta̠ i kö̀ppéeke.",
            "imagen": "quelite_paso7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Be’ ö̀kö niè̠wa íñe̠.",
            "imagen": "chicha_pejibaye1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Bulé be’ dikó alé wö̀tskélor",
            "imagen": "chicha_pejibaye2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ièka̠ be’ kö́ ttö̀ië́ule sík wa e’ a̠.",
            "imagen": "chicha_pejibaye3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tsé̠mi be’ ák kì̠. Wèe.",
            "imagen": "chicha_pejibaye4",
            "audio": "avespasos1"
        },
        {
            "nombre": "Dikó chka ièka̠ne̠ be’ kö́ ttö́iulewa̠ sík wa e’ a̠",
            "imagen": "chicha_pejibaye5",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tchéka̠ be’ ù̠ a̠, tö̀ki̠ be’ i a̠ ö̀kö tchéka̠ wö̀ti̠è̠ka̠",
            "imagen": "chicha_pejibaye6",
            "audio": "avespasos1"
        },
        {
            "nombre": "I wö̀ti̠è̠ke ulà wa.̠",
            "imagen": "chicha_pejibaye7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Mé̠a̠t e’s kö́ bikèwa̠ sík wa.",
            "imagen": "chicha_pejibaye8",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ikuwö̀ sí shtèke.",
            "imagen": "chichas1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ijkuö̀ tchée.",
            "imagen": "chichas2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ikuwö̀ ë̀ yétsa̠ .",
            "imagen": "chichas3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Wö̀tsée be’ ù̠ a̠.",
            "imagen": "chichas4",
            "audio": "avespasos1"
        },
        {
            "nombre": "A̠ di’ téka̠ mè̠a̠t e’s.",
            "imagen": "chichas5",
            "audio": "avespasos1"
        },
        {
            "nombre": "Buléña̠jkë ta̠ be’ i dí tétsa̠ .",
            "imagen": "chichas6",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tchéka̠ne̠ kö́ ttö̀ië́ulewa̠ sík wa e’ a̠ .",
            "imagen": "chichas7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kö̀bikéwa̠ sík wa mè̠a̠t e’s.",
            "imagen": "chichas8",
            "audio": "avespasos1"
        },
        {
            "nombre": "Dö̀ páiki ta̠ ikuwö̀ shölö̀ne.",
            "imagen": "chichas9",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ta̠ be’ i wée.",
            "imagen": "chichas10",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ chkà bikéwa̠ be’ kál sík wa, e’s i aléka̠ .",
            "imagen": "chichas11",
            "audio": "avespasos1"
        },
        {
            "nombre": "I alìne̠ ta̠ be’ i tchéka̠ ù̠ a̠ .",
            "imagen": "chichas12",
            "audio": "avespasos1"
        },
        {
            "nombre": "I pà sé̠ne̠ ta̠ be’ i yèlor ù̠ a̠ .",
            "imagen": "chichas13",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tulése̠ e’s.",
            "imagen": "chichas14",
            "audio": "avespasos1"
        },
        {
            "nombre": "Páiki be’ sík yétsa̠ i trée.",
            "imagen": "chichas15",
            "audio": "avespasos1"
        },
        {
            "nombre": "I bikèkewa̠ i sík e’ ë̀ wa erë sík tsi̠wa.",
            "imagen": "chichas16",
            "audio": "avespasos1"
        },
        {
            "nombre": "Dö̀ka̠ tché̠k ta̠ be’ i sík yélor.",
            "imagen": "chichas17",
            "audio": "avespasos1"
        },
        {
            "nombre": "Idapàwe̠ka̠ wö̀chka si’ èt. E’ta̠ i síne.",
            "imagen": "chichas18",
            "audio": "avespasos1"
        },
        {
            "nombre": "Be’ ö̀ kö nie̠wa íñe̠ .",
            "imagen": "chicha_maiz1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ikuwö̀ sí wö̀tsée be’.",
            "imagen": "chicha_maiz2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Ni̠è̠wa̠ di’ a̠ íñe.",
            "imagen": "chicha_maiz3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Bulé be’ i tsé̠mi̠ ák ki̠, wée.",
            "imagen": "chicha_maiz4",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ chkà bikèwa̠ sík wa ña̠ ’ ie.",
            "imagen": "chicha_maiz5",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ aléka̠ be’ u̠ ̀ ki̠ .",
            "imagen": "chicha_maiz6",
            "audio": "avespasos1"
        },
        {
            "nombre": "I alìne̠ ta̠ be’ iéwa.",
            "imagen": "chicha_maiz7",
            "audio": "avespasos1"
        },
        {
            "nombre": "Sík yélor.",
            "imagen": "chicha_maiz8",
            "audio": "avespasos1"
        },
        {
            "nombre": "Wö̀tié̠e̠ be’ rö kúshki wa.",
            "imagen": "chicha_maiz9",
            "audio": "avespasos1"
        },
        {
            "nombre": "Tchékane̠ be’ rö u̠ ̀ a̠ ö̀kö ta̠ , shu̠ ̀ tie̠ ̀ka̠ .",
            "imagen": "chicha_maiz10",
            "audio": "avespasos1"
        },
        {
            "nombre": "Kö̀bikéwa̠ sík wa.",
            "imagen": "chicha_maiz11",
            "audio": "avespasos1"
        },
        {
            "nombre": "I wö̀ katúule ajkuö̀ klétsa̠ wée.",
            "imagen": "chocolate1",
            "audio": "avespasos1"
        },
        {
            "nombre": "Di’ ba mé̠ka̠ wö̀bichö̀we̠e̠.",
            "imagen": "chocolate2",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ a̠ be’ tsuru’ chka iéka̠ .",
            "imagen": "chocolate3",
            "audio": "avespasos1"
        },
        {
            "nombre": "Shu̠ ̀ tuèke kúshki wa.",
            "imagen": "chocolate4",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’ yèke cha̠mu̠ ̀ katú ruwà ie.",
            "imagen": "chocolate5",
            "audio": "avespasos1"
        },
        {
            "nombre": "Dikó alíule ièkeka̠ kö́ a̠ .",
            "imagen": "pejibaye1",
            "audio": "avespasos1"
        },
        {
            "nombre": "E’s i kichà ièkeka̠ bö́ tsa̠ díwö èt.",
            "imagen": "pejibaye2",
            "audio": "avespasos1"
        },
        {
            "nombre": "Mìka̠ sö i ñèketche, e’ta̠ i jkuö̀ tchéeke ë̀.",
            "imagen": "pejibaye3",
            "audio": "avespasos1"
        }
